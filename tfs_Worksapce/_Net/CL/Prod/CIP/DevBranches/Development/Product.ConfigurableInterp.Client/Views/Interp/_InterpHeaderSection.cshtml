@model Interp

@using (Html.BeginForm())
{
    <div class="container" style="margin-top: 45px">
        <div class="col-sm-6">
            <table class="Caption TableSummary margin-8 float-left">
                <tr>
                    <td>@Html.LabelFor(x => x.Outline, new { @class = "Bold" })</td>
                    <td>
                        <label>
                            @Html.DisplayTextFor(x => x.Outline)
                        </label>
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(x => x.Category, new { @class = "Bold" })</td>
                    <td>
                        <label>
                            @Html.DisplayTextFor(x => x.Category)
                        </label>
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(x => x.Subcategory, new { @class = "Bold" })</td>
                    <td>
                        <label>
                            @Html.DisplayTextFor(x => x.Subcategory)
                        </label>
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(x => x.Level, new { @class = "Bold" })</td>
                    <td>
                        <label>
                            @Html.DisplayTextFor(x => x.Level)
                        </label>
                    </td>
                </tr>
            </table>
        </div>
        <div class="col-sm-6">
            <table class="Caption TableSummary margin-8 float-right">
                <tr>
                    <td>@Html.LabelFor(x => x.Username, new { @class = "Bold" })</td>
                    <td>
                        <label>
                            @Html.DisplayTextFor(x => x.Username)
                        </label>
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(x => x.MaintenanceDate, new { @class = "Bold" })</td>
                    <td>
                        <label>
                            @Html.DisplayTextFor(x => x.MaintenanceDate)
                        </label>
                    </td>
                </tr>
                @if (Model.Status == "Active")
                {
                    <tr>
                        <td>@Html.LabelFor(x => x.ActivatedDate, new { @class = "Bold" })</td>
                        <td>
                            <label>
                                @Html.DisplayTextFor(x => x.ActivatedDate)
                            </label>
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
    <div id="dialog-confirm" title="Warning">
        <p>
            <div id="dialog-confirm-text"></div>
        </p>
    </div>
}

